# Docker environment variables
# Copy this file to .env and fill in your values
# NEVER commit .env to source control!

# Azure App Configuration (optional - falls back to local config)
AZURE_APP_CONFIG_CONNECTIONSTRING=

# Application Insights (optional - telemetry disabled if empty)
APPLICATIONINSIGHTS_CONNECTIONSTRING=

# Redis Configuration (used when CacheProvider=Redis)
REDIS_CONNECTIONSTRING=localhost:6379
